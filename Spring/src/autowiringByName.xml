<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans  
               http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id="id1" class="com.bridgelabz.autowiringByName.Categories"
		autowire="byName">
		<property name="name" value="General Books" />
	</bean>
	<bean id="bk" class="com.bridgelabz.autowiringByName.Book">
		<property name="bookname" value=" wings of fire" />
		<property name="bookprice" value=" 300" />
	</bean>
</beans>